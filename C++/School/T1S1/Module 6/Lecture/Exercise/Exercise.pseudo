START
    FOR n = 0 to 2
        DISPLAY n + " "
    END FOR
END

START
    SET n = 5
    WHILE n > 1
        DISPLAY n + " "
        DECREMENT n
    END WHILE
END

START
    FOR n = 1 to 10
        DISPLAY n + " "
    END FOR
END

START
    SET n = 7
    DO
        DISPLAY n + " "
        DECREMENT n
    WHILE n >= 1
    END DO
END

START
    SET n = 1
    WHILE n > 1
        DISPLAY n + " "
        DECREMENT n
    END WHILE
END