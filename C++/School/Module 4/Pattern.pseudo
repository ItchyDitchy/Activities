START
    SET x = 1
    WHILE x <= 5
        SET y = 1
        INCREMENT y
        WHILE y <= x
            OUTPUT "*"
        ENDWHILE
        OUTPUT "\n"
        INCREMENT x
    ENDWHILE
END


